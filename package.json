{
  "name": "graph-fns",
  "version": "0.4.0-1",
  "description": "A utility library for working with graphs.",
  "repository": "github:haydn/graph-fns",
  "keywords": [
    "graph",
    "utility",
    "graph theory",
    "networks"
  ],
  "main": "cjs/index.js",
  "author": "Haydn Ewers",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "yarn build:declaration && yarn build:cjs && yarn build:umd && yarn build:esm",
    "build:declaration": "tsc index.ts --downlevelIteration --declaration --emitDeclarationOnly && flowgen index.d.ts -o index.js.flow",
    "build:cjs": "tsc index.ts --downlevelIteration --target ES2015 --module CommonJS --outDir cjs",
    "build:umd": "tsc index.ts --downlevelIteration --target ES2015 --module UMD --outDir umd",
    "build:esm": "tsc index.ts --target ESNext --module ESNext --outDir esm",
    "release": "np",
    "test": "prettier -c **/*.ts **/*.md && tsc && tape tests/**/*.js",
    "version": "yarn build"
  },
  "files": [
    "index.d.ts",
    "index.js.flow",
    "cjs/index.js",
    "umd/index.js",
    "esm/index.js",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^14.14.12",
    "@types/tape": "^4.13.0",
    "flowgen": "^1.13.0",
    "np": "^7.0.0",
    "prettier": "^2.2.1",
    "tape": "^5.0.1",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "never",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
