// @flow

declare export type Graph = {
    size: number;
    adjacencyMatrix: Array<Array<number>>;
};
declare export type D3Graph = {
    nodes: Array<{
        id: string;
    }>;
    links: Array<{
        source: string;
        target: string;
    }>;
};
declare export function toD3(graph: Graph): D3Graph;
declare export function fromD3(graph: D3Graph): Graph;
declare export function isCyclic(graph: Graph): boolean;
declare export function topologicalSort(graph: Graph): Array<number>;
